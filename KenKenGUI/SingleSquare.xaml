<Window x:Class="KenKenGUI.SingleSquare"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KenKenGUI"
        mc:Ignorable="d"
        Title="New Region" Height="180" Width="138.095" ResizeMode="NoResize">
    <Canvas>
        <ComboBox x:Name="cbTarget" Height="53" Width="65" Canvas.Left="35" Canvas.Top="43" RenderTransformOrigin="1.025,0.589" FontSize="16" FontWeight="Bold" SelectedIndex="0">
            <ComboBoxItem Content="1"/>
            <ComboBoxItem Content="2"/>
            <ComboBoxItem Content="3"/>
            <ComboBoxItem Content="4"/>
            <ComboBoxItem Content="5"/>
            <ComboBoxItem Content="6"/>
        </ComboBox>
        <Button Content="OK" RenderTransformOrigin="5.326,8.414" Canvas.Left="10" Canvas.Top="118" Width="52" Click="Button_Click"/>
        <Button Content="Cancel" RenderTransformOrigin="5.326,8.414" Canvas.Left="70" Canvas.Top="118" Width="52"/>
    </Canvas>
</Window>
